<?xml version="1.0" encoding="utf-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <template class="PhoshWifiNetworkRow" parent="GtkListBoxRow">
    <property name="visible">True</property>
    <child>
      <object class="GtkBox">
        <property name="visible">True</property>
        <property name="orientation">horizontal</property>
        <property name="spacing">6</property>
        <property name="margin-top">6</property>
        <property name="margin-end">6</property>
        <property name="margin-bottom">6</property>
        <property name="margin-start">6</property>
        <child>
          <object class="GtkBox">
            <property name="visible">True</property>
            <property name="orientation">horizontal</property>
            <child>
              <object class="GtkImage" id="wifi_icon">
                <property name="visible">True</property>
                <property name="icon-name">network-wireless-signal-none</property>
              </object>
            </child>
            <child>
              <object class="GtkImage" id="encrypted_icon">
                <property name="visible">True</property>
                <property name="valign">end</property>
                <property name="pixel-size">8</property>
                <property name="icon-name">network-wireless-encrypted</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="ssid_label">
            <property name="visible">True</property>
            <property name="label">Wifi AP</property>
          </object>
        </child>
        <child>
          <object class="GtkImage" id="active_indicator">
            <property name="visible">True</property>
            <property name="icon-name">object-select</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
